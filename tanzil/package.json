	"scripts": {
		"build": "cross-env NODE_ENV=production tsc && vite build",
		"check": "tsc",
		"dev": "vite",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:fix": "eslint . --ext .ts,.tsx --fix",
