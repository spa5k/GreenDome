		"storybook": " cross-env STORYBOOK=true concurrently \"pnpm:watch:*\"",
		"build-storybook": "concurrently \"pnpm:build:*\"",
		"build:css": "npx tailwindcss -i ./src/main.css -o ./public/tailwind.css",
		"build:storybook": "storybook build",
		"watch:css": "npx tailwindcss -i ./src/main.css -o ./public/tailwind.css --watch",
